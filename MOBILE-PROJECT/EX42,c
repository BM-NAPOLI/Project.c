#include<stdio.h>

int main()
{
    
    int L , C ;
    int I , J ; 
    
    printf("ENTREZ LR NOMBRES DES LINGES : ");
    scanf("%d",&L);
    printf("ENTREZ LR NOMBRES DU COLONES : ");
    scanf("%d",&C);
    printf("\n");
    
    for ( I = 1 ; I <= L ; I ++ ) {
    for ( J = 1 ; J <= L ; J ++ ) {
        printf("  $  ");
    }    
        printf("\n");
    }
    
    
    
    
    
    
    
    
    
    
    
    
    return 0;
}